{
  "name": "sokeio/framework",
  "description": "a Laravel-based web application development package that provides support for developing modules, plugins, themes, livewire, and shortcodes.",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://github.com/sokeio/framework",
  "authors": [
    {
      "name": "Nguyen Van Hau",
      "email": "nguyenvanhau.soft@gmail.com",
      "role": "Developer",
      "homepage": "https://github.com/devhau"
    }
  ],
  "require": {
    "livewire/livewire": "^3.5.18",
    "laravel/socialite": "^5.16.0"
  },
  "require-dev": {},
  "autoload": {
    "files": [
      "helpers/constraint.php",
      "helpers/function.php"
    ],
    "psr-4": {
      "Sokeio\\Livewire\\": "features/livewire/",
      "Sokeio\\Marketplate\\": "features/marketplate/",
      "Sokeio\\MediaStorage\\": "features/media-storage/",
      "Sokeio\\Core\\": "features/core/",
      "Sokeio\\Menu\\": "features/menu/",
      "Sokeio\\Theme\\": "features/theme/",
      "Sokeio\\UI\\": "features/ui/",
      "Sokeio\\Widget\\": "features/widget/",
      "Sokeio\\Seeders\\": "database/seeders/",
      "Sokeio\\Database\\Factories\\": "database/factories/",
      "Sokeio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sokeio\\SokeioServiceProvider"
      ],
      "aliases": {}
    }
  },
  "config": {
    "sort-features": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
